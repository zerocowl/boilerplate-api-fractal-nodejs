{
  "name": "boilerplate-nodejs",
  "version": "0.0.1",
  "description": "Node API boilerplate",
  "main": "server.js",
  "scripts": {
    "start": "babel-node src/server.js",
    "build": "./node_modules/.bin/babel src --out-dir dist/ && cp package.json ./dist/",
    "start-node": "nodemon --exec babel-node src/server.js",
    "clean:build": "rm -rf dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerocowl/boilerplate-nodejs.git"
  },
  "keywords": [
    "zerocool",
    "boilerplate",
    "nodejs",
    "express",
    "zero",
    "node"
  ],
  "author": "zerocool",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zerocowl/boilerplate-nodejs/issues"
  },
  "homepage": "https://github.com/zerocowl/boilerplate-nodejs#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1"
  },
